<Window x:Class="LatihanLKSDesktop.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LatihanLKSDesktop"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800">
    <Grid>
        <Grid Background="#FF86B4FF" HorizontalAlignment="Left" Width="192">
            <Label Content="Admin" Margin="63,26,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold" HorizontalAlignment="Left" Width="66"/>
            <Image Height="100" Margin="46,65,46,0" VerticalAlignment="Top" Source="/software-engineer.png"/>
            <Button Content="Kelola User" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" FontSize="16" Click="OpenUser"/>
            <Button Content="Kelola Laporan" HorizontalAlignment="Center" Margin="0,247,0,0" VerticalAlignment="Top" FontSize="16" Click="OpenLaporan"/>
            <Button Content="Logout" HorizontalAlignment="Center" Margin="0,277,0,0" VerticalAlignment="Top" FontSize="16" Click="Logout"/>
        </Grid>
        <Grid Margin="192,0,0,0">
            <Frame x:Name="MainContent" Content=""/>
        </Grid>

    </Grid>
</Window>
